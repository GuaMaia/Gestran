program Candidato_GustavoMaia;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {Form1},
  Umodulo in 'Umodulo.pas' {Dm: TDataModule},
  UConexao in 'UConexao.pas' {DataModule2: TDataModule};

